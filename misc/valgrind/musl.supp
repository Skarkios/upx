# musl malloc
{
   musl-malloc-enframe
   Memcheck:Cond
   fun:enframe
}
{
   musl-malloc-get_nominal_size
   Memcheck:Cond
   fun:get_nominal_size
   fun:__libc_free
}

# these are only needed for older musl versions

{
   musl-malloc-mal0_clear
   Memcheck:Cond
   fun:mal0_clear
   fun:calloc
}

{
   musl-string-memchr
   Memcheck:Cond
   fun:memchr
}
{
   musl-string-stpcpy
   Memcheck:Cond
   fun:stpcpy
}
{
   musl-string-strchrnul
   Memcheck:Cond
   fun:strchrnul
}
{
   musl-string-strlen
   Memcheck:Cond
   fun:strlen
}
